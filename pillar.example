vim:
  managed_vimrc: true
  allow_localrc: true
  config:
    syntax: 'on'
    colors: desert
  settings:
    et:
    cin:
    ru:
    bs: indent,eol,start
    showcmd:
    showmatch:
    smartcase:
    incsearch:
    autowrite:
    hidden:
    mouse: a
    number:
    backspace: 2
    noerrorbells:
    novisualbell:
    background: dark
    ai:
    si:
    cindent:
    tabstop: 8
    softtabstop: 8
    shiftwidth: 8
    nowrap:
    swapsync: ''
    nocompatible:
    paste:
    filetype: off


  lets:
    &colorcolumn: join(range(121,121),",")
